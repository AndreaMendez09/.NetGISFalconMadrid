@model Net_Gis_Falcon.Respuestum

@{
    ViewData["Title"] = "Delete";
}
<h1>Eliminar</h1>

<h3>¿Estás seguro de que quieres eliminar esto? </h3>
<div>
    <h4>Respuestas</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Cuerpo de la respuesta</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CuerpoRespuesta)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Principal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Principal)
        </dd>
        <dt class="col-sm-2">Nivel</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NivelNavigation.DescripcionPregunta)
        </dd>
        <dt class="col-sm-2">Respuesta del padre</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RespuestaPadreNavigation.CuerpoRespuesta)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdRespuesta" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
