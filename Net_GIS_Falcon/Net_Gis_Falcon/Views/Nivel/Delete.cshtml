@model Net_Gis_Falcon.Nivel

@{
    ViewData["Title"] = "Delete";
}
<h1>Eliminar</h1>

<h3>¿Estás seguro de que quieres eliminar esto?</h3>
<div>
    <h4>Nivel</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">Descripcion </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescripcionPregunta)
        </dd>
        <dt class = "col-sm-2">Nivel</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nivel1)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdPregunta" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
