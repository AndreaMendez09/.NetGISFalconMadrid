@model Net_Gis_Falcon.Nivel

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>
<h4>Nivel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdPregunta" />
            <div class="form-group">
                <label asp-for="DescripcionPregunta" class="control-label">Descripción</label>
                <input asp-for="DescripcionPregunta" class="form-control" />
                <span asp-validation-for="DescripcionPregunta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nivel1" class="control-label">Nivel</label>
                <input asp-for="Nivel1" class="form-control" />
                <span asp-validation-for="Nivel1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>


