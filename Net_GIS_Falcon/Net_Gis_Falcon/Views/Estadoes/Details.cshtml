@model Net_Gis_Falcon.Estado

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Estado</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Nombre de estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreEstado)
        </dd>
        <dt class = "col-sm-2">
           Es final ?
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Esfinal)
        </dd>
        <dt class = "col-sm-2">
           Color de estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ColorEstado)
        </dd>
        <dt class = "col-sm-2">
           Padre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PadreNavigation.NombreEstado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdEstado">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
