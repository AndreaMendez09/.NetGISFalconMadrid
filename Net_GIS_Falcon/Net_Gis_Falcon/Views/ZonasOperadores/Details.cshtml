@model Net_Gis_Falcon.OperadoresZona

@{
    ViewData["Title"] = "Details";

}
<h1>Detalles</h1>

<div>
    <h4>Operadores de Zona</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Operador</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OperadorNavigation.Apellido)
        </dd>
        <dt class="col-sm-2">Zona </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ZonaNavigation.NombreZona)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Volver a la lista</a>
</div>
