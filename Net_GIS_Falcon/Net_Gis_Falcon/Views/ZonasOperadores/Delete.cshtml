@model Net_Gis_Falcon.OperadoresZona

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estás seguro de que quieres eliminar esto? </h3>
<div>
    <h4>Operadores de Zona</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Operadores</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OperadorNavigation.Apellido)
        </dd>
        <dt class="col-sm-2">Zonas </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ZonaNavigation.NombreZona)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Operador" />
        <input type="hidden" asp-for="Zona" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>

